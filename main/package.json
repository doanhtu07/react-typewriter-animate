{
  "name": "react-typewriter-animate",
  "description": "A React Typewriter that works on blocks of texts.",
  "author": "Anh Tu Do",
  "license": "MIT",
  "keywords": [
    "react",
    "components",
    "ui",
    "react typewriter",
    "react typewriter effect",
    "typewriter",
    "effect",
    "animation",
    "front-end"
  ],
  "version": "1.2.3",
  "private": false,
  "main": "dist/index.js",
  "types": "dist/@types/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/doanhtu07/react-typewriter-animate.git"
  },
  "dependencies": {
    "@types/jquery": "^3.5.9",
    "clsx": "^1.1.1",
    "jquery": "^3.6.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "typescript": "^4.4.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "tsc && cp ./src/*.css ./dist",
    "watch": "tsc -w",
